        "/api/posts/search": {
            "get": {
                "tags": [
                    "Posts"
                ],
                "summary": "Search posts",
                "description": "Search posts based on query parameters",
                "operationId": "0fac29de97628c7fccbf66c1e9e10798",
                "parameters": [
                    {
                        "name": "Accept",
                        "in": "header",
                        "description": "Media type expected by the client",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "example": "application/json"
                        }
                    },
                    {
                        "name": "query",
                        "in": "query",
                        "description": "Search term for the posts",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "rowsPerPage",
                        "in": "query",
                        "description": "Number of rows per page",
                        "required": false,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "sortBy",
                        "in": "query",
                        "description": "Sort by fields (id, created_at, published_at)",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "enum": [
                                "id",
                                "created_at",
                                "published_at"
                            ]
                        }
                    },
                    {
                        "name": "sortDirection",
                        "in": "query",
                        "description": "Sort direction (asc or desc)",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "enum": [
                                "asc",
                                "desc"
                            ]
                        }
                    },
                    {
                        "name": "paginate",
                        "in": "query",
                        "description": "Flag to paginate the results",
                        "required": false,
                        "schema": {
                            "type": "boolean"
                        }
                    },
                    {
                        "name": "status",
                        "in": "query",
                        "description": "Filter by post status",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "enum": [
                                "DRAFT",
                                "PUBLISHED"
                            ]
                        }
                    },
                    {
                        "name": "published_at (start_date)",
                        "in": "query",
                        "description": "Filter by published start date",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "date"
                        }
                    },
                    {
                        "name": "published_at (end_date)",
                        "in": "query",
                        "description": "Filter by published end date",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "date"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "List of posts matching the search criteria"
                    },
                    "422": {
                        "description": "Invalid input"
                    },
                    "401": {
                        "description": "Unauthorize - User does not exists",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "The following user does not exists."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": {
                    "sanctum": {
                        "bearerAuth": []
                    }
                }
            }
        },
